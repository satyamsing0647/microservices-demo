package com.coforge.training.shopstop.model;

import jakarta.persistence.GeneratedValue;
import jakarta.persistence.GenerationType;
import jakarta.persistence.Id;

/*
* Date   : 29 Nov 2024
* Time   : 1:01:25â€¯pm
* Author : Satyam.3.Singh 
* Project: product-service
